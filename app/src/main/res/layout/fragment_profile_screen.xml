<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <include layout="@layout/common_header_loginsignup" />

        <ScrollView
            android:background="@color/loginheadercolor"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

            <androidx.cardview.widget.CardView
                android:id="@+id/loginCardView"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp">
                    <TextView
                        android:id="@+id/test"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Profile"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_topheading_textsize"/>

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/profileName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/dp_20"
                        android:layout_marginBottom="@dimen/dp_20"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Profile"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_maintitle_textsize" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:id="@+id/taxableVehicleInformationMC"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="2dp"
                        app:strokeWidth="1dp"
                        android:padding="@dimen/dp_10"
                        app:strokeColor="@color/blacklignt">

                        <LinearLayout
                            android:padding="@dimen/dp_10"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:fontFamily="@font/roboto_medium"
                                android:textSize="@dimen/page_title_textsize"
                                android:gravity="center|start"
                                android:id="@+id/profileEditMyProfile"
                                android:drawablePadding="@dimen/dp_10"
                                android:padding="@dimen/dp_10"
                                android:drawableStart="@drawable/editprofile"
                                android:text="Edit my profile"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:id="@+id/reportingsuspendedMC"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="2dp"
                        app:strokeWidth="1dp"
                        android:padding="@dimen/dp_10"
                        app:strokeColor="@color/blacklignt">

                        <LinearLayout
                            android:padding="@dimen/dp_10"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:fontFamily="@font/roboto_medium"
                                android:textSize="@dimen/page_title_textsize"
                                android:gravity="center|start"
                                android:id="@+id/profileChangePassword"
                                android:drawablePadding="@dimen/dp_10"
                                android:padding="@dimen/dp_10"
                                android:drawableStart="@drawable/changepassword"
                                android:text="Change Password"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:id="@+id/priorYearSuspendedMC"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="2dp"
                        app:strokeWidth="1dp"
                        android:padding="@dimen/dp_10"
                        app:strokeColor="@color/blacklignt">

                        <LinearLayout
                            android:padding="@dimen/dp_10"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:fontFamily="@font/roboto_medium"
                                android:textSize="@dimen/page_title_textsize"
                                android:gravity="center|start"
                                android:id="@+id/profileMyTransactions"
                                android:drawablePadding="@dimen/dp_10"
                                android:padding="@dimen/dp_10"
                                android:drawableStart="@drawable/mytransaction"
                                android:text="My Transactions"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:id="@+id/soldDestroyedStolenVehicleMC"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="2dp"
                        app:strokeWidth="1dp"
                        android:padding="@dimen/dp_10"
                        app:strokeColor="@color/blacklignt">

                        <LinearLayout
                            android:padding="@dimen/dp_10"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:fontFamily="@font/roboto_medium"
                                android:textSize="@dimen/page_title_textsize"
                                android:gravity="center|start"
                                android:id="@+id/profileChangeLoginMail"
                                android:drawablePadding="@dimen/dp_10"
                                android:padding="@dimen/dp_10"
                                android:drawableStart="@drawable/changeloginmail"
                                android:text="Change login mail"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:id="@+id/lowMileageVehicleMC"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_20"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="2dp"
                        app:strokeWidth="1dp"
                        android:padding="@dimen/dp_10"
                        app:strokeColor="@color/blacklignt">

                        <LinearLayout
                            android:padding="@dimen/dp_10"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:fontFamily="@font/roboto_medium"
                                android:textSize="@dimen/page_title_textsize"
                                android:gravity="center|start"
                                android:id="@+id/profileTaxPreparerInformation"
                                android:drawablePadding="@dimen/dp_10"
                                android:padding="@dimen/dp_10"
                                android:drawableStart="@drawable/taxprepayerinformation"
                                android:text="Tax Preparer Information"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>


                </LinearLayout>

            </androidx.cardview.widget.CardView>
        </ScrollView>
    </LinearLayout>


</FrameLayout>