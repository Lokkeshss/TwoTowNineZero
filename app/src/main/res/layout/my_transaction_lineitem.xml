<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <com.google.android.material.card.MaterialCardView
        android:id="@+id/taxableVehicleMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        app:cardBackgroundColor="@color/businesslistbackground"
        app:cardCornerRadius="8dp"
        app:cardElevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="@color/blacklignt"
        app:strokeWidth="1dp">

        <LinearLayout
            android:paddingStart="@dimen/dp_10"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/myTransactionBusinessName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/roboto_medium"
                        android:text="JOHN LLC"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_title_textsize" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Business Name"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/page_title_textsize" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="@dimen/dp_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/roboto_medium"
                        android:text="Date of Payment"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_title_textsize" />

                    <TextView
                        android:gravity="center|start"
                        android:id="@+id/myTransactionDateofPayment"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Business Name"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/page_value_textsize" />
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/roboto_medium"
                        android:text="Form Type"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_title_textsize" />

                    <TextView
                        android:gravity="center|start"
                        android:id="@+id/myTransactionFormType"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Business Name"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/page_value_textsize" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="@dimen/dp_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/roboto_medium"
                        android:text="Transaction ID"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_title_textsize" />

                    <TextView
                        android:gravity="center|start"
                        android:id="@+id/myTransactionTransactionID"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Business Name"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/page_value_textsize" />
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/roboto_medium"
                        android:text="Voucher No"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_title_textsize" />

                    <TextView
                        android:gravity="center|start"
                        android:id="@+id/myTransactionVoucherNo"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Business Name"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/page_value_textsize" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="@dimen/dp_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/roboto_medium"
                        android:text="Payment Status"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_title_textsize" />

                    <TextView
                        android:gravity="center|start"
                        android:id="@+id/myTransactionPaymentStatus"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Business Name"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/page_value_textsize" />
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/roboto_medium"
                        android:text="Amount"
                        android:textColor="@color/black"
                        android:textSize="@dimen/page_title_textsize" />

                    <TextView
                        android:gravity="center|start"
                        android:id="@+id/myTransactionAmount"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/roboto_regular"
                        android:text="Business Name"
                        android:textColor="@color/next"
                        android:textSize="@dimen/page_value_textsize" />
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>

    </com.google.android.material.card.MaterialCardView>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/taxableVehicleMain" />



</androidx.constraintlayout.widget.ConstraintLayout>