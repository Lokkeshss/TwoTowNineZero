<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dashboard.bottomnavigation.filling.taxyear_and_forms.TaxYearAndFormFragment">

    <LinearLayout
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <include layout="@layout/common_header_loginsignup" />

    <androidx.core.widget.NestedScrollView
        android:layout_weight="1"
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <androidx.cardview.widget.CardView
        android:id="@+id/loginCardView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingStart="20dp"
        android:paddingEnd="20dp">
    <TextView
        android:id="@+id/test"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Add New Filing"
        android:textColor="@color/black"
        android:textSize="@dimen/page_topheading_textsize"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <include
                android:layout_gravity="center"
                android:id="@+id/dutyCycleTimeLayout"
                layout="@layout/progress_bar_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                />
<!--    app:progressMaxValue="@{analysis.getWeekDutyMax() + 0}"
                app:progressValue="@{analysis.getWeekDutyTotal() + 0}"
                app:progressText="@{util.spannedProgressTime(util.millisToHHmm(analysis.getWeekDutyTotal()),@string/weekHours)}" -->
            <TextView
                android:layout_marginStart="@dimen/dp_10"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:fontFamily="@font/roboto_bold"
                android:text="Tax Years &amp; Forms"
                android:textColor="@color/black"
                android:textSize="@dimen/page_maintitle_textsize"/>
        </LinearLayout>




        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp"
            app:strokeWidth="1dp"
            app:strokeColor="@color/blacklignt">

                <LinearLayout
                    android:visibility="visible"
                    android:id="@+id/businessProfileChild"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_10"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter the business name"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/fillingBusinessName"
                            android:textAllCaps="true"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_10"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select Form Type"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/fillingFormType"
                            android:drawableEnd="@drawable/edittextdropdownicon"
                            android:textAllCaps="true"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:id="@+id/TIfillingTaxYear"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_10"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select Tax Year"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/fillingTaxYear"
                            android:textAllCaps="true"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TIfillingFirstUserMonth"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_10"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select First Used Month"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/fillingFirstUserMonth"
                            android:textAllCaps="true"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TTfillingAmendmentMonth"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_10"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select Amendment Month "
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/fillingAmendmentMonth"
                            android:textAllCaps="true"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TIfillingYourIncomeTax"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_10"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select Month Your Income Tax Year Ends"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/fillingYourIncomeTax"
                            android:textAllCaps="true"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <LinearLayout
                        android:id="@+id/LLSwitch"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">



                    <TextView
                        android:textSize="@dimen/page_title_textsize"
                        android:fontFamily="@font/roboto_medium"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:drawablePadding="10dp"
                        android:textColor="@color/textcolor"
                        android:text="Final Return"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                    <androidx.appcompat.widget.SwitchCompat
                        android:layout_marginTop="@dimen/dp_5"
                        android:id="@+id/fillingFinalReturn"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textSize="@dimen/page_title_textsize"
                        android:fontFamily="@font/roboto_medium"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:drawablePadding="10dp"
                        android:textColor="@color/textcolor"
                        android:text="(Click Final Return,if you no longer have vehicles to file a tax)"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textSize="@dimen/page_title_textsize"
                        android:fontFamily="@font/roboto_medium"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:drawablePadding="10dp"
                        android:textColor="@color/textcolor"
                        android:text="Address Change"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                    <androidx.appcompat.widget.SwitchCompat
                        android:id="@+id/fillingAddressChange"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                    </LinearLayout>

                </LinearLayout>


        </com.google.android.material.card.MaterialCardView>




    </LinearLayout>

    </androidx.cardview.widget.CardView>

    </androidx.core.widget.NestedScrollView>

        <LinearLayout
            android:padding="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="center"
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:visibility="gone"
                android:id="@+id/fillingCancel"
                android:fontFamily="@font/roboto_medium"
                android:textSize="12sp"
                android:layout_marginEnd="20dp"
                android:drawablePadding="20dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:padding="15dp"
                android:background="@drawable/button_withwhite_roundcoronor_color"
                android:drawableStart="@drawable/addbusiness_cancelbuttonicon"
                android:text="Cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/fillingSubmit"
                android:fontFamily="@font/roboto_medium"
                android:textSize="@dimen/page_button_textsize"
                android:drawablePadding="20dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:padding="15dp"
                android:textColor="@color/white"
                android:background="@drawable/btn_bg_round_cornor_with_moreround_cornor"
                android:drawableEnd="@drawable/nexticonwhite"
                android:text="Submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

    </LinearLayout>

</FrameLayout>