<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dashboard.bottomnavigation.home.FilingFilterFragment">

    <LinearLayout
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <include layout="@layout/common_header_loginsignup" />

    <ScrollView
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <androidx.cardview.widget.CardView
        android:id="@+id/loginCardView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingStart="20dp"
        android:paddingEnd="20dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_30"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:fontFamily="@font/roboto_bold"
            android:gravity="center"
            android:text="My Filing-Return Status"
            android:textColor="@color/black"
            android:textSize="@dimen/page_topheading_textsize"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/fragmentBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="10dp"
            android:src="@drawable/fragmentbackarrow"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginEnd="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_20"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="From Date"
            app:hintTextColor="@color/lightGray">

            <EditText
                android:drawablePadding="@dimen/dp_10"
                android:drawableStart="@drawable/calender"
                android:drawableEnd="@drawable/edittextdropdownicon"
                android:textAllCaps="true"
                android:id="@+id/filterFromDate"
                android:fontFamily="@font/roboto_regular"
                style="@style/loginTextInputEditText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                android:inputType="text"
                android:text="" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginEnd="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_20"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="To Date"
            app:hintTextColor="@color/lightGray">

            <EditText
                android:drawablePadding="@dimen/dp_10"
                android:drawableStart="@drawable/calender"
                android:drawableEnd="@drawable/edittextdropdownicon"
                android:textAllCaps="true"
                android:id="@+id/filterToDate"
                android:fontFamily="@font/roboto_regular"
                style="@style/loginTextInputEditText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                android:inputType="text"
                android:text="" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginEnd="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_20"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Keywords"
            app:hintTextColor="@color/lightGray">

            <EditText
                android:drawableEnd="@drawable/edittextdropdownicon"
                android:textAllCaps="true"
                android:id="@+id/filterKeyWord"
                android:fontFamily="@font/roboto_regular"
                style="@style/loginTextInputEditText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                android:inputType="text"
                android:text="" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginEnd="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_20"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint=" Select Category"
            app:hintTextColor="@color/lightGray">

            <EditText
                android:drawableEnd="@drawable/edittextdropdownicon"
                android:textAllCaps="true"
                android:id="@+id/filterSearchCategory"
                android:fontFamily="@font/roboto_regular"
                style="@style/loginTextInputEditText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                android:inputType="text"
                android:text="" />
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:id="@+id/filterSearch"
            android:fontFamily="@font/roboto_medium"
            android:textSize="@dimen/page_button_textsize"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:layout_marginStart="@dimen/dp_20"
            android:drawablePadding="20dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:padding="15dp"
            android:textColor="@color/white"
            android:background="@drawable/btn_bg_round_cornor_with_moreround_cornor"
            android:drawableStart="@drawable/buttonsearchicon"
            android:text="Search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    </androidx.cardview.widget.CardView>
    </ScrollView>
    </LinearLayout>


</FrameLayout>