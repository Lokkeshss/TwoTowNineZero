<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dashboard.bottomnavigation.filling.form_summary.FormSummaryBusinessInformationFragment">

    <LinearLayout
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <include layout="@layout/common_header_loginsignup" />

    <androidx.core.widget.NestedScrollView
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <androidx.cardview.widget.CardView
        android:id="@+id/loginCardView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingStart="20dp"
        android:paddingEnd="20dp">
    <TextView
        android:id="@+id/test"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Add New Filing"
        android:textColor="@color/black"
        android:textSize="@dimen/page_topheading_textsize"/>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:id="@+id/taxableVehicleInformationMC"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp"
            app:strokeWidth="1dp"
            android:padding="@dimen/dp_10"
            app:strokeColor="@color/blacklignt">

            <LinearLayout
                android:padding="@dimen/dp_10"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:fontFamily="@font/roboto_medium"
                        android:textSize="@dimen/page_maintitle_textsize"
                        android:layout_weight="1"
                        android:id="@+id/taxableVehicleInformation"
                        android:drawablePadding="@dimen/dp_10"
                        android:padding="@dimen/dp_10"
                        android:drawableStart="@drawable/businessinfoblueicon"
                        android:text="Business Information"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                    <ImageView
                        android:id="@+id/businessInfoEdit"
                        android:layout_gravity="center"
                        android:padding="@dimen/dp_10"
                        android:src="@drawable/editiconblack"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                </LinearLayout>

                <TextView
                    android:fontFamily="@font/roboto_medium"
                    android:textSize="@dimen/page_title_textsize"
                    android:drawablePadding="@dimen/dp_10"
                    android:drawableStart="@drawable/businessnameicon"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:text="Business Profile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Business Name"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessName"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="EIN"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessEin"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Business Type"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessType"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:fontFamily="@font/roboto_medium"
                    android:textSize="@dimen/page_title_textsize"
                    android:drawablePadding="@dimen/dp_10"
                    android:drawableStart="@drawable/calliconsmall"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:text="Contact Info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Address"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessAddress"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Phone No"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessPhoneNo"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Email ID"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessEmailId"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:fontFamily="@font/roboto_medium"
                    android:textSize="@dimen/page_title_textsize"
                    android:drawablePadding="@dimen/dp_10"
                    android:drawableStart="@drawable/signinauthicon"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textColor="@color/black"
                    android:text="Signing Authority Info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Name"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessauthName"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Title"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessTitle"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_20"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Phone No"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:textColor="@color/black"
                        android:enabled="false"
                        android:id="@+id/formSummaryBusinessauthPhoneNo"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>




                <LinearLayout
                    android:id="@+id/thridPartyDesignLL"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:fontFamily="@font/roboto_medium"
                        android:textSize="@dimen/page_title_textsize"
                        android:drawablePadding="@dimen/dp_10"
                        android:drawableStart="@drawable/signinauthicon"
                        android:layout_marginTop="@dimen/dp_10"
                        android:textColor="@color/black"
                        android:text="Third Party Designee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Name"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:textColor="@color/black"
                            android:enabled="false"
                            android:id="@+id/formSummaryThirdPartyName"
                            android:textAllCaps="true"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Phone No"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:textColor="@color/black"
                            android:enabled="false"
                            android:id="@+id/formSummaryPhoneNo"
                            android:textAllCaps="true"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:inputType="phone"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>



                </LinearLayout>

            </LinearLayout>


        </com.google.android.material.card.MaterialCardView>


    </LinearLayout>
    </androidx.cardview.widget.CardView>
    </androidx.core.widget.NestedScrollView>
    </LinearLayout>


</FrameLayout>