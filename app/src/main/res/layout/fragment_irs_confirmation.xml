<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dashboard.bottomnavigation.filling.isr_confirmation.IrsConfirmationFragment">

    <LinearLayout
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <include layout="@layout/common_header_loginsignup" />

    <androidx.core.widget.NestedScrollView
        android:layout_weight="1"
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <androidx.cardview.widget.CardView
        android:id="@+id/loginCardView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingStart="20dp"
        android:paddingEnd="20dp">
    <TextView
        android:id="@+id/test"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Add New Filing"
        android:textColor="@color/black"
        android:textSize="@dimen/page_topheading_textsize"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include
            android:layout_gravity="center"
            android:id="@+id/dutyCycleTimeLayout"
            layout="@layout/progress_bar_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            />
        <!--    app:progressMaxValue="@{analysis.getWeekDutyMax() + 0}"
                        app:progressValue="@{analysis.getWeekDutyTotal() + 0}"
                        app:progressText="@{util.spannedProgressTime(util.millisToHHmm(analysis.getWeekDutyTotal()),@string/weekHours)}" -->
        <TextView
            android:layout_marginStart="@dimen/dp_10"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:fontFamily="@font/roboto_bold"
            android:text="IRS confirmation"
            android:textColor="@color/black"
            android:textSize="@dimen/page_maintitle_textsize"/>
    </LinearLayout>

        <TextView
            android:fontFamily="@font/roboto_medium"
            android:textSize="@dimen/page_title_textsize"
            android:textColor="@color/textcolor"
            android:text="IRS is currently processing your form.Please allow a few minutes before checking the status of your form  "
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.google.android.material.card.MaterialCardView

            android:layout_marginTop="@dimen/dp_10"
            android:id="@+id/irsPaymentOptionCreditCardMain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp"
            app:strokeWidth="1dp"
            app:strokeColor="@color/blacklignt">
            <LinearLayout
                android:padding="@dimen/dp_5"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:fontFamily="@font/roboto_regular"
                    android:textSize="@dimen/page_title_textsize"
                    android:textColor="@color/black"
                    android:background="@drawable/textview_lightblue_roundcornor"
                    android:padding="@dimen/dp_10"
                    android:text="Submission Fee Details"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Transaction ID"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:enabled="false"
                        android:id="@+id/transactionID"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputEditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Voucher No"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:enabled="false"
                        android:id="@+id/transactionVoucherNo"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputEditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Payment Status"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:enabled="false"
                        android:id="@+id/transactionPaymentStatus"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputEditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    style="@style/loginTextInputLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Paid Amount"
                    app:hintTextColor="@color/lightGray">

                    <EditText
                        android:enabled="false"
                        android:id="@+id/transactionPaidAmount"
                        android:textAllCaps="true"
                        android:fontFamily="@font/roboto_regular"
                        style="@style/loginTextInputEditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:text="" />
                </com.google.android.material.textfield.TextInputLayout>




            </LinearLayout>


        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/irsPaymentOptionCreditCardfeeDetails"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp"
            app:strokeWidth="1dp"
            app:strokeColor="@color/blacklignt">

   <LinearLayout
       android:padding="@dimen/dp_10"
       android:layout_width="match_parent"
       android:orientation="vertical"
       android:layout_height="wrap_content">



        <TextView
            android:fontFamily="@font/roboto_regular"
            android:textSize="@dimen/page_title_textsize"
            android:textColor="@color/black"
            android:background="@drawable/textview_lightblue_roundcornor"
            android:padding="@dimen/dp_10"
            android:text="Submission Fee Details"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>


        <TextView
            android:textSize="@dimen/page_value_textsize"
            android:fontFamily="@font/roboto_regular"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_gravity="center"
            android:drawablePadding="@dimen/dp_10"
            android:text="Choose ype of tax -> Business tax payments"
            android:drawableStart="@drawable/irsconfirmation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:textSize="@dimen/page_value_textsize"
            android:fontFamily="@font/roboto_regular"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_gravity="center"
            android:drawablePadding="@dimen/dp_10"
            android:text="Choose form 2290 series heavy high way vehicle taxes"
            android:drawableStart="@drawable/irsconfirmation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:textSize="@dimen/page_value_textsize"
            android:fontFamily="@font/roboto_regular"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_gravity="center"
            android:drawablePadding="@dimen/dp_10"
            android:text="Specify current year or prior year due as per your case"
            android:drawableStart="@drawable/irsconfirmation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:textSize="@dimen/page_value_textsize"
            android:fontFamily="@font/roboto_regular"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_gravity="center"
            android:drawablePadding="@dimen/dp_10"
            android:text="Then enter the tax amount-> choose debit/credit and enter the card  details to complete the payment"
            android:drawableStart="@drawable/irsconfirmation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

   </LinearLayout>
        </com.google.android.material.card.MaterialCardView>


    </LinearLayout>
    </androidx.cardview.widget.CardView>
    </androidx.core.widget.NestedScrollView>
        <TextView
            android:textSize="@dimen/page_button_textsize"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_10"
            android:id="@+id/transactionSubmit"
            android:fontFamily="@font/roboto_medium"
            android:gravity="center"
            android:layout_gravity="center"
            android:padding="15dp"
            android:textColor="@color/white"
            android:background="@drawable/btn_bg_round_cornor_with_moreround_cornor"
            android:text="Go to my filings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

</FrameLayout>