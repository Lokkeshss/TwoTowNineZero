<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dashboard.bottomnavigation.filling.form_summary.FormSummaryTaxYearInformationFragment">

    <LinearLayout
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <include layout="@layout/common_header_loginsignup" />

    <androidx.core.widget.NestedScrollView
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <androidx.cardview.widget.CardView
        android:id="@+id/loginCardView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingStart="20dp"
        android:paddingEnd="20dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/roboto_bold"
                android:gravity="center"
                android:text="Add New Filing"
                android:textColor="@color/black"
                android:textSize="@dimen/page_topheading_textsize"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/taxyearBack"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:padding="10dp"
                android:src="@drawable/fragmentbackarrow"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:id="@+id/taxableVehicleInformationMC"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        app:cardCornerRadius="8dp"
        app:cardElevation="2dp"
        app:strokeWidth="1dp"
        android:padding="@dimen/dp_10"
        app:strokeColor="@color/blacklignt">

    <LinearLayout
        android:padding="@dimen/dp_10"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="1"
            android:id="@+id/taxableVehicleInformation"
            android:drawablePadding="@dimen/dp_10"
            android:padding="@dimen/dp_10"
            android:drawableStart="@drawable/taxyearinformationicon"
            android:text="Tax Year Information"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <ImageView
            android:id="@+id/taxYearInformationEdit"
            android:layout_gravity="center"
            android:padding="@dimen/dp_10"
            android:src="@drawable/editiconblack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:layout_marginStart="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_20"
        style="@style/loginTextInputLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Form Type"
        app:hintTextColor="@color/lightGray">

        <EditText
            android:textColor="@color/black"
            android:enabled="false"
            android:id="@+id/formSummarytaxyearInfoFormType"
            android:textAllCaps="true"
            android:fontFamily="@font/roboto_regular"
            android:textSize="14sp"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
            android:inputType="text"
            android:text="" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/formSummarytaxyearTaxYearTI"
        android:layout_marginStart="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_20"
        style="@style/loginTextInputLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Tax Year"
        app:hintTextColor="@color/lightGray">

        <EditText
            android:textColor="@color/black"
            android:enabled="false"
            android:id="@+id/formSummarytaxyearTaxYear"
            android:textAllCaps="true"
            android:fontFamily="@font/roboto_regular"
            android:textSize="14sp"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
            android:inputType="text"
            android:text="" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/formSummaryFirstUsedMonthTI"
        android:layout_marginStart="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_20"
        style="@style/loginTextInputLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="First Used Month"
        app:hintTextColor="@color/lightGray">

        <EditText
            android:textColor="@color/black"
            android:enabled="false"
            android:id="@+id/formSummaryFirstUsedMonth"
            android:textAllCaps="true"
            android:fontFamily="@font/roboto_regular"
            android:textSize="14sp"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
            android:inputType="text"
            android:text="" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/formSummaryAmendmentMonthTI"
        android:layout_marginStart="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_20"
        style="@style/loginTextInputLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Amendment Month"
        app:hintTextColor="@color/lightGray">

        <EditText
            android:textColor="@color/black"
            android:enabled="false"
            android:id="@+id/formSummaryAmendmentMonth"
            android:textAllCaps="true"
            android:fontFamily="@font/roboto_regular"
            android:textSize="14sp"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
            android:inputType="text"
            android:text="" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/formSummaryMonthIncomeTaxTI"
        android:layout_marginStart="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_20"
        style="@style/loginTextInputLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Month Your Income Tax Year Ends"
        app:hintTextColor="@color/lightGray">

        <EditText
            android:textColor="@color/black"
            android:enabled="false"
            android:id="@+id/formSummaryMonthIncomeTax"
            android:textAllCaps="true"
            android:fontFamily="@font/roboto_regular"
            android:textSize="14sp"
            style="@style/loginTextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890. "
            android:inputType="text"
            android:text="" />
    </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>
    </com.google.android.material.card.MaterialCardView>
    </LinearLayout>
    </androidx.cardview.widget.CardView>
    </androidx.core.widget.NestedScrollView>
    </LinearLayout>


</FrameLayout>