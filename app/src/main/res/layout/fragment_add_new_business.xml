<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dashboard.bottomnavigation.business.AddNewBusinessFragment">

    <LinearLayout
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <include layout="@layout/common_header_loginsignup" />

    <androidx.core.widget.NestedScrollView
        android:layout_weight="1"
        android:background="@color/loginheadercolor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <androidx.cardview.widget.CardView
        android:id="@+id/loginCardView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingStart="@dimen/dp_10"
        android:paddingEnd="@dimen/dp_10">
    <TextView
        android:id="@+id/test"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginBottom="@dimen/dp_10"
        android:fontFamily="@font/roboto_bold"
        android:text="Add New Business"
        android:textColor="@color/black"
        android:textSize="@dimen/page_topheading_textsize"/>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp"
            app:strokeWidth="1dp"
            app:strokeColor="@color/blacklignt">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/taxableVehicleMain"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="2dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="@color/blacklignt">
                    <TextView
                        android:fontFamily="@font/roboto_regular"
                        android:textSize="@dimen/page_maintitle_textsize"
                        android:textColor="@color/textcolor"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:drawablePadding="20dp"
                        android:padding="10dp"
                        android:text="Business Profile"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:drawableStartCompat="@drawable/addnewbusiness_profile"
                        app:drawableEndCompat="@drawable/edittextdropdownicon" />
                </com.google.android.material.card.MaterialCardView>


                <LinearLayout
                    android:visibility="visible"
                    android:id="@+id/businessProfileChild"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter the business name"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/businessProfileBusinessName"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:inputType="textCapWords"
                            />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter EIN"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/businessProfileEin"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="1234567890-"
                            android:inputType="phone"
                            android:maxLength="10"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <TextView
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:drawablePadding="10dp"
                        android:textColor="#808080"
                        android:drawableStart="@drawable/addnewbusiness_questionmark"
                        android:text="Enter 9 digit number.Eg-12-3456789"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>


                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_25"
                        android:layout_marginBottom="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select Business Type"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/businessProfileBusinessType"
                            android:drawableEnd="@drawable/edittextdropdownicon"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890 "
                            android:inputType="textCapWords"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>



                </LinearLayout>

            </LinearLayout>


        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp"
            app:strokeWidth="1dp"
            app:strokeColor="@color/blacklignt">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/signingAuthInfoMain"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="2dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="@color/blacklignt">
                    <TextView
                        android:fontFamily="@font/roboto_regular"
                        android:textSize="@dimen/page_maintitle_textsize"
                        android:textColor="@color/textcolor"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:drawablePadding="20dp"
                        android:padding="10dp"
                        android:text="Signing Authority Information"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:drawableStartCompat="@drawable/addnewbusiness_signinauth"
                        app:drawableEndCompat="@drawable/edittextdropdownicon" />
                </com.google.android.material.card.MaterialCardView>

                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/signingAuthInfoChild"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter the name"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/signingAuthInfoName"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "
                            android:inputType="textCapWords"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter the day time phone"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:maxLength="12"
                            android:id="@+id/signingAuthInfoPhone"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="1234567890+-"
                            android:inputType="phone"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>


                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        android:layout_marginBottom="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select Title"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/signingAuthInfoTitle"
                            android:drawableEnd="@drawable/edittextdropdownicon"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890@."
                            android:inputType="textCapWords"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>



                </LinearLayout>

            </LinearLayout>


        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp"
            app:strokeWidth="1dp"
            app:strokeColor="@color/blacklignt">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/contactInfoMain"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="2dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="@color/blacklignt">
                    <TextView
                        android:textSize="@dimen/page_maintitle_textsize"
                        android:fontFamily="@font/roboto_regular"
                        android:textColor="@color/textcolor"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:drawablePadding="20dp"
                        android:padding="10dp"
                        android:text="Contact Information"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:drawableStartCompat="@drawable/addnewbusiness_contactinfo"
                        app:drawableEndCompat="@drawable/edittextdropdownicon" />
                </com.google.android.material.card.MaterialCardView>


                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/contactInfoChild"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter the Address"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/contactInfoAddress"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890@."
                            android:inputType="textCapWords"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select Country"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/contactInfoCounty"
                            android:drawableEnd="@drawable/edittextdropdownicon"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890@."
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Select State/Province"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/contactInfostate"
                            android:drawableEnd="@drawable/edittextdropdownicon"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890@."
                            android:inputType="text"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter City"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/contactInfocity"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890@."
                            android:inputType="textCapWords"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter Zip Code"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:maxLength="10"
                            android:id="@+id/contactInfoZipCode"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="1234567890"
                            android:inputType="phone"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter Phone Number"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:maxLength="12"
                            android:id="@+id/contactInfoPhoneNumber"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="1234567890+-"
                            android:inputType="phone"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        android:layout_marginBottom="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter Email Address"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/contactInfoEmailAddress"
                            android:fontFamily="@font/roboto_regular"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890@."
                            android:inputType="textEmailAddress"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                </LinearLayout>

            </LinearLayout>


        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp"
            app:strokeWidth="1dp"
            app:strokeColor="@color/blacklignt">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/thirdPartydesignMain"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="2dp"
                    app:strokeWidth="1dp"
                    app:strokeColor="@color/blacklignt">
                    <TextView
                        android:fontFamily="@font/roboto_regular"
                        android:textSize="@dimen/page_maintitle_textsize"
                        android:textColor="@color/textcolor"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:drawablePadding="20dp"
                        android:padding="10dp"
                        android:text="Third Party Designee"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:drawableStartCompat="@drawable/addnewbusiness_thirdparty"
                        app:drawableEndCompat="@drawable/edittextdropdownicon" />
                </com.google.android.material.card.MaterialCardView>


                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/thirdPartydesignChild"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_marginTop="@dimen/dp_20"
                        android:layout_marginStart="@dimen/dp_5"
                        android:lineSpacingExtra="8sp"
                        android:textColor="@color/textcolor"
                        android:fontFamily="@font/roboto_medium"
                        android:textSize="12sp"
                        android:text="@string/addbusiness_signauth"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>


                    <CheckBox
                        android:id="@+id/thirdPartydesign_checkbox"
                        android:layout_marginTop="@dimen/dp_30"
                        android:layout_gravity="center"
                        android:fontFamily="@font/roboto_medium"
                        android:textColor="@color/black"
                        android:text="Third Party Designee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>


                    <LinearLayout
                        android:visibility="gone"
                        android:id="@+id/thirdPartydesign_childtwo"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter Third Party Designee Name"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:id="@+id/ThirdPartyDesignName"
                            android:fontFamily="@font/roboto_regular"
                            android:textSize="14sp"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "
                            android:inputType="textCapWords"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:layout_marginTop="@dimen/dp_20"
                        android:layout_marginBottom="@dimen/dp_20"
                        style="@style/loginTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Enter Third Party Designee Phone Number"
                        app:hintTextColor="@color/lightGray">

                        <EditText
                            android:maxLength="12"
                            android:id="@+id/ThirdPartyDesignPhonenumber"
                            android:fontFamily="@font/roboto_regular"
                            android:textSize="14sp"
                            style="@style/loginTextInputEditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="1234567890+"
                            android:inputType="phone"
                            android:text="" />
                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

                </LinearLayout>

            </LinearLayout>


        </com.google.android.material.card.MaterialCardView>


        <LinearLayout
            android:background="@color/transparant"
            android:padding="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_20"
            android:gravity="center"
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/addNewBusinessCancel"
                android:fontFamily="@font/roboto_medium"
                android:textSize="@dimen/page_button_textsize"
                android:layout_marginEnd="20dp"
                android:drawablePadding="20dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:padding="15dp"
                android:background="@drawable/button_withwhite_roundcoronor_color"
                android:drawableStart="@drawable/addbusiness_cancelbuttonicon"
                android:text="Cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/addNewBusinessSubmit"
                android:fontFamily="@font/roboto_medium"
                android:textSize="@dimen/page_button_textsize"
                android:layout_marginStart="20dp"
                android:drawablePadding="20dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:padding="15dp"
                android:textColor="@color/white"
                android:background="@drawable/btn_bg_round_cornor_with_moreround_cornor"
                android:drawableStart="@drawable/addbusiness_submitbuttonicon"
                android:text="Submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </LinearLayout>
    </androidx.cardview.widget.CardView>
    </androidx.core.widget.NestedScrollView>

    </LinearLayout>

</FrameLayout>